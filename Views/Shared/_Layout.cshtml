<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"] - MicroFIN</title>
    <meta content="" name="description">
    <meta content="" name="keywords">
    
    <!-- Google Fonts -->
    <link href="https://fonts.gstatic.com" rel="preconnect">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Nunito:300,300i,400,400i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    <link href="~/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/lib/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="~/lib/simple-datatables/style.css" rel="stylesheet">
    <link rel="stylesheet" href="https://cdn.datatables.net/1.13.1/css/jquery.dataTables.min.css"/>
    <link rel="stylesheet" href="https://cdn.datatables.net/1.13.1/css/dataTables.bootstrap5.min.css"/>
    <link rel="stylesheet" href="https://cdn.datatables.net/responsive/2.4.0/css/responsive.bootstrap5.min.css"/>
    <link rel="stylesheet" href="https://cdn.datatables.net/buttons/2.3.2/css/buttons.bootstrap5.min.css"/>

    
    <link rel="stylesheet" href="~/css/style.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true"/>

    @* <link rel="stylesheet" href="~/MicroFIN.styles.css" asp-append-version="true"/> *@
</head>
<body>
<partial name="_CookieConsentPartial"/>
  <!-- ======= Header ======= -->
  <header id="header" class="header fixed-top d-flex align-items-center">

    <div class="d-flex align-items-center justify-content-between">
      <a href="/" class="logo d-flex align-items-center">
        <img src="/images/logo.png" alt="">
        <span class="d-none d-lg-block"><small>JAI BHOLE FINANCE</small></span>
      </a>
      <i class="bi bi-list toggle-sidebar-btn"></i>
    </div><!-- End Logo -->

    <div class="search-bar">
      <form class="search-form d-flex align-items-center" method="POST" action="#">
        <input type="number" id="txtsearchdairynumber" value="" placeholder="Enter Dairy Number"  title="Enter Dairy Number">
        <button type="button" title="Search" onclick="onClickSearch(this)"><i class="bi bi-search"></i></button>
      </form>
    </div><!-- End Search Bar -->

    <nav class="header-nav ms-auto">
      <ul class="d-flex align-items-center">
        <li class="nav-item d-block d-lg-none">
          <a class="nav-link nav-icon search-bar-toggle " href="#">
            <i class="bi bi-search"></i>
          </a>
        </li><!-- End Search Icon-->
          <li class="nav-item pe-3">
              <a class="nav-link nav-profile d-flex align-items-center pe-0" asp-action="Setting" asp-controller="Admin">
                  <span class="d-md-block ps-2">@User.Identity.Name</span>
              </a>
          </li>
          <li class="nav-item dropdown pe-3">
             
              <a class="nav-link nav-profile d-flex align-items-center pe-0" asp-action="logout" asp-controller="account">
                  <i class="bi bi-box-arrow-right" style="font-size: 1.5rem;"></i>
              </a>
              @* <a class="nav-link nav-profile d-flex align-items-center pe-0" href="#" data-bs-toggle="dropdown"> *@
              @*   <img src="assets/img/profile-img.jpg" alt="Profile" class="rounded-circle"> *@
              @*   <span class="d-none d-md-block dropdown-toggle ps-2">K. Anderson</span> *@
              @* </a><!-- End Profile Iamge Icon --> *@
          </li><!-- End Profile Nav -->
      </ul>
    </nav><!-- End Icons Navigation -->

</header><!-- End Header -->

  <!-- ======= Sidebar ======= -->
  <aside id="sidebar" class="sidebar">
  <ul class="sidebar-nav" id="sidebar-nav">
      <li class="nav-item">
          <a class="nav-link" asp-area="" asp-controller="Admin" asp-action="Index"><i class="bi bi-grid"></i><span>Dashboard</span></a>
      </li>
      <li class="nav-item">
          <a class="nav-link collapsed" asp-area="" asp-controller="Admin" asp-action="Dairy"><i class="bi bi-book"></i><span>Dairies</span></a>
      </li>
      <li class="nav-item">
          <a class="nav-link collapsed" asp-area="" asp-controller="Admin" asp-action="Customers"><i class="bi bi-people"></i><span>Customers</span></a>
      </li>
      <li class="nav-item">
          <a class="nav-link collapsed" asp-area="" asp-controller="Admin" asp-action="Agents"><i class="bi bi-person-badge-fill"></i><span>Agents</span></a>
      </li>
      <li class="nav-item">
          <a class="nav-link collapsed" asp-area="" asp-controller="Admin" asp-action="Expences"><i class="bi bi-currency-rupee"></i><span>Expences</span></a>
      </li>
      <li class="nav-item">
          <a class="nav-link collapsed" asp-area="" asp-controller="Report" asp-action="Index"><i class="bi bi-stack"></i><span>Reports</span></a>
      </li>
  </ul>

  </aside><!-- End Sidebar-->
<main id="main" class="main">
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

</main>
  <!-- ======= Footer ======= -->
  <footer id="footer" class="footer">
    <div class="copyright">
      &copy; Copyright <strong><span>JAI BHOLE FINANCE</span></strong>. All Rights Reserved
    </div>
    <div class="credits">
     <small> Designed by <a href="https://kodonic.com/">Kodonic Digitech Private Limited</a></small>
    </div>
  </footer><!-- End Footer -->

  <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

<div class="loaderbody" id="loaderbody">
    <div class="loader"></div>
</div>
